{
	"name": "reshaper",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "npm run gen-proto && tsc && vite build",
		"dev": "npm run gen-proto && vite",
		"format": "npm run format:prettier && npm run format:proto",
		"format:prettier": "prettier --write .",
		"format:proto": "buf format src/ReShaperDocument.proto -o src/ReShaperDocument.proto",
		"gen-proto": "pbjs -t static-module -w es6 -o src/ReShaperDocument.js src/ReShaperDocument.proto && pbts -o src/ReShaperDocument.d.ts src/ReShaperDocument.js",
		"postinstall": "npm run gen-proto",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"test": "vitest"
	},
	"prettier": "@allejo/prettier-config",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.4.2",
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-regular-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@react-hooks-library/core": "^0.5.1",
		"@types/jmespath": "^0.15.0",
		"@types/lodash": "^4.14.196",
		"@types/node": "^20.5.4",
		"autoprefixer": "^10.4.14",
		"dayjs": "^1.11.9",
		"immer": "^10.0.2",
		"jmespath": "^0.16.0",
		"lodash": "^4.17.21",
		"postcss": "^8.4.27",
		"postcss-extend-rule": "^4.0.0",
		"postcss-nesting": "^12.0.1",
		"protobufjs": "^7.2.5",
		"protobufjs-cli": "^1.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"tailwindcss": "^3.3.3",
		"type-fest": "^4.2.0",
		"use-immer": "^0.9.0",
		"usehooks-ts": "^2.9.1"
	},
	"devDependencies": {
		"@allejo/prettier-config": "^1.0.3",
		"@bufbuild/buf": "^1.26.1",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"@vitejs/plugin-react": "^4.0.1",
		"eslint": "^8.44.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.1",
		"prettier": "^3.0.1",
		"typescript": "^5.0.2",
		"vite": "^4.4.0",
		"vitest": "^0.34.3"
	}
}
